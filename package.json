  "description": "The Verto Components library for React",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "repository": "https://github.com/useverto/ui",
  "author": "The Verto Team",
  "license": "MIT",
  "private": false,
  "files": [
    "dist"
  ],
